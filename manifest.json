{
  "manifest_version": 2,
  "name": "amazonAccountManager",
  "version": "1.0.0.0",
  "description": "存储Amzon账户信息",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [ "https://www.amazon.com/*" ],
      "js": [ "js/content-script.js" ],
      "run_at": "document_start"
    }
  ]
}